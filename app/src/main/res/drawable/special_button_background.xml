<?xml version="1.0" encoding="utf-8"?>
<selector xmlns:android="http://schemas.android.com/apk/res/android">
    <item android:state_pressed="true">
        <shape android:shape="oval">
            <solid android:color="?attr/specialButton1BackgroundPressedColor" />
            <stroke android:width="?attr/specialButton1BackgroundStrokeWidth" android:color="?attr/specialButton1BackgroundPressedStrokeColor" />
        </shape>
    </item>
    <item android:state_pressed="false">
        <shape android:shape="oval">
            <solid android:color="?attr/specialButton1BackgroundUnpressedColor" />
            <stroke android:width="?attr/specialButton1BackgroundStrokeWidth" android:color="?attr/specialButton1BackgroundUnpressedStrokeColor" />
        </shape>
    </item>
</selector>